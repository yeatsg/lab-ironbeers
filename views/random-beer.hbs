<div>
    <ul>
         {{#each randomizedBeer}}
        <li>{{> beerpartial this }}
            <h3>Food Pairing</h3>
            <ul>
                {{#each this.food_pairing}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
            <h3>Brewer's Tips</h3>
            <p>{{this.brewers_tips}}</p>
        </li>
         {{/each}}
    </ul>
</div>